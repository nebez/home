[user]
	useConfigOnly = true
[alias]
	s = status -s
	pr = "!hub pull-request -b $1"
	last = log -1 HEAD
	proon = fetch origin --prune
	aliases = "!git config -l | grep alias | cut -c 7-"
[core]
	excludesfile = /Users/nebez.briefkani/.gitignore_global
	pager = diff-so-fancy | less --tabs=4 -RFX
	editor = nano
[diff-so-fancy]
	markEmptyLines = false
[hub]
	protocol = https
[includeIf "gitdir:/Volumes/Development/src/github.com/lightspeed/"]
	path = /Volumes/Development/src/github.com/lightspeed/gitconfig.inc
[includeIf "gitdir:/Volumes/Development/src/github.com/lightspeedretail/"]
        path = /Volumes/Development/src/github.com/lightspeedretail/gitconfig.inc
[includeIf "gitdir:/Volumes/Development/src/github.com/LightspeedServices/"]
        path = /Volumes/Development/src/github.com/LightspeedServices/gitconfig.inc
[includeIf "gitdir:/Volumes/Development/src/github.com/nebez/"]
        path = /Volumes/Development/src/github.com/nebez/gitconfig.inc
[includeIf "gitdir:/Volumes/Development/src/github.com/latitude85/"]
        path = /Volumes/Development/src/github.com/latitude85/gitconfig.inc
[pull]
	rebase = false
