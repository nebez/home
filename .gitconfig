[user]
	name = Nebez Briefkani
	email = nebez.briefkani@lightspeedhq.com
[alias]
	s = status -s
	pr = "!hub pull-request -c -b $1"
	last = log -1 HEAD
	proon = fetch origin --prune
	aliases = "!git config -l | grep alias | cut -c 7-"
[core]
	excludesfile = /Users/nebez.briefkani/.gitignore_global
	pager = diff-so-fancy | less --tabs=4 -RFX
	editor = nano
[diff-so-fancy]
	markEmptyLines = false
[credential]
	helper = osxkeychain
[hub]
	protocol = https
